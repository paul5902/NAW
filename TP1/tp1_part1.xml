<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE formules SYSTEM "C:\Users\utilisateur\Documents\tp1_dtd.dtd">
<formules>
	<constantes>
		<const nom="A" valeur="1"/>
		<const nom="B" valeur="7"/>
		<const nom="C" valeur="50"/>
	</constantes>
	<exps>
		<exp nom="exp1">
			<op symbole="mult">
				<op symbole="plus">
					<var nom="A"/>
					<var nom="B"/>
				</op>
				<op symbole="moins">
					<var nom="A"></var>
					<var nom="B"></var>
				</op>
			</op>
		</exp>
		<exp nom="exp2">
			<op symbole="mult">
				<op symbole="moins">
					<op symbole="moins">
						<var nom="A"/>
						<var nom="A"/>
					</op>
					<var nom="C"/>
				</op>	
				<var nom="A"/>
			</op>
		</exp>
		<exp nom="exp3">
			<op symbole="mult">
				<op symbole="mult">
					<op symbole="div">
						<var nom="B"/>
						<var nom="A"/>
					</op>
					<var nom="C"/>
				</op>
				<var nom="C"/>
			</op>
		</exp>
	</exps>
</formules>
